mn":0}},{"source":"main.css","original":{"line":224,"column":0},"generated":{"line":224,"column":0}},{"source":"main.css","original":{"line":225,"column":0},"generated":{"line":225,"column":0}},{"source":"main.css","original":{"line":226,"column":0},"generated":{"line":226,"column":0}},{"source":"main.css","original":{"line":227,"column":0},"generated":{"line":227,"column":0}},{"source":"main.css","original":{"line":228,"column":0},"generated":{"line":228,"column":0}},{"source":"main.css","original":{"line":229,"column":0},"generated":{"line":229,"column":0}},{"source":"main.css","original":{"line":230,"column":0},"generated":{"line":230,"column":0}},{"source":"main.css","original":{"line":231,"column":0},"generated":{"line":231,"column":0}},{"source":"main.css","original":{"line":232,"column":0},"generated":{"line":232,"column":0}},{"source":"main.css","original":{"line":233,"column":0},"generated":{"line":233,"column":0}},{"source":"main.css","original":{"line":234,"column":0},"generated":{"line":234,"column":0}},{"source":"main.css","original":{"line":235,"column":0},"generated":{"line":235,"column":0}},{"source":"main.css","original":{"line":236,"column":0},"generated":{"line":236,"column":0}},{"source":"main.css","original":{"line":237,"column":0},"generated":{"line":237,"column":0}},{"source":"main.css","original":{"line":238,"column":0},"generated":{"line":238,"column":0}},{"source":"main.css","original":{"line":239,"column":0},"generated":{"line":239,"column":0}},{"source":"main.css","original":{"line":240,"column":0},"generated":{"line":240,"column":0}},{"source":"main.css","original":{"line":241,"column":0},"generated":{"line":241,"column":0}},{"source":"main.css","original":{"line":242,"column":0},"generated":{"line":242,"column":0}},{"source":"main.css","original":{"line":243,"column":0},"generated":{"line":243,"column":0}}],"sources":{"main.css":"/****   main.css   ****/\n\n:root {\n  --bg-color: #fff;\n  --font-color: #333;\n  --highlight-color: rgba(0,0,0,.12);\n  --action-color: #eb5757;\n  --header-height: 3.6rem;\n  --large-margin: 5rem;\n}\n\n/* #region RESET */\nhtml {\n  box-sizing: border-box;\n  font-size: 62.5%; /* sets 1rem to 10px by default */\n  font-family: 'Robotica', sans-serif;\n}\n*,\n*:before,\n*:after {\n  box-sizing: inherit;\n  margin: 0;\n  padding: 0;\n  /*outline: 1px solid rgba(0,0,0,.25);*/\n}\nbody {\n  font-size: 1.6rem; /* 16px */\n  line-height: 1.6rem; /* center text */\n  color: #000;\n  background: var(--bg-color);\n}\nul {\n  list-style: none;\n}\na {\n  text-decoration: none;\n  color: initial;\n}\n/*#endregion RESET */\n\n/*#region GLOBAL CLASSES */\n.container { /* for header and other top-level page elements */\n  padding: 0 1rem;\n}\n.section { /* for dashboard sections */\n  padding-top: 1.6rem;\n  padding-bottom: 1.6rem;\n}\n.heading {\n  text-align: center;\n  font-size: 3.6rem;  /* same as header height */\n  line-height: 3.6rem;\n  padding-bottom: 1.8rem; /* half the header height */\n  font-weight: 500;\n}\n.subheading {\n  font-size: 1.8rem;\n  line-height: 1.8rem;\n}\n.btn {\n  cursor: pointer;\n  padding: .3rem;\n}\n.active {\n  display: initial !important;\n}\n.checked {\n  text-decoration: line-through;\n}\n.noselect {\n  -webkit-touch-callout: none; /* iOS Safari */\n    -webkit-user-select: none; /* Safari */\n     -khtml-user-select: none; /* Konqueror HTML */\n       -moz-user-select: none; /* Firefox */\n        -ms-user-select: none; /* Internet Explorer/Edge */\n            user-select: none; /* supported by chrome and opera */\n}\n/*#endregion GLOBAL CLASSES */\n\n/*#region HEADER */\n.page-header {\n  width: 100%;\n  height: var(--header-height);\n  line-height: var(--header-height);\n  background: var(--font-color);\n  color: var(--bg-color);\n  display: flex;\n  justify-content: space-between;\n  box-shadow: 0 0 2rem rgba(0,0,0,.2);\n}\n.nav-list {\n  display: flex;\n  justify-content: flex-end;\n}\n.nav-list a {\n  padding: .7rem 1rem;\n}\n.nav-link {\n  color: var(--bg-color);\n}\n.nav-link:hover,\n.nav-link:active,\n.nav-link:focus {\n  text-decoration: underline;\n}\n/*#endregion HEADER */\n\n/*#region DASHBOARD */\n.dashboard-container {\n  position: relative;\n  width: 100%;\n  height: calc(100vh - 3.6rem);\n  display: flex;\n}\n\n/*#region SIDEBAR */\n.sidebar-content {\n  position: relative;\n  flex: 1;  /* 25% width */\n  max-width: 25%;\n  background: var(--highlight-color);\n  box-shadow: 0 0 2rem rgba(0,0,0,.15);\n  max-height: calc(100vh - 3.6rem);\n  max-width: 100%;\n}\n.sidebar-header .heading {\n  min-height: 10rem;\n}\n.sidebar-actions-list {\n  display: flex;\n  flex-direction: column;\n  margin-top: var(--large-margin);\n  border: 1px solid var(--bg-color);\n}\n.sidebar-link {\n  background: var(--font-color);\n  color: var(--bg-color);\n  padding: 1.6rem;\n  width: 100%;\n  max-width: 100%;\n  display: inline-block;\n  text-align: center;\n  border-bottom: 1px solid var(--bg-color);\n  cursor: pointer;\n}\n.sidebar-link:hover,\n.sidebar-link:active,\n.sidebar-link:focus {\n  background: #111;\n  border-left: 7px solid var(--action-color);\n  margin-left: -4px; /* keep link center */\n}\n\n/*#region NOTES-SIDEBAR */\n.sidebar-notes {\n  margin-top: var(--large-margin);\n  max-width: 100%;\n}\n.notes-header {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  line-height: 3.6rem;\n  margin-right: 5.4rem;\n}\n.notes-header .heading {\n  line-height: 3.6rem;\n  font-size: 3.6rem;\n  padding: 1.8rem 0;\n}\n.notes-container {\n  border: 1px solid rgba(0,0,0,.2);\n  margin-top: .6rem;\n  overflow: auto;\n}\n.btn-add-note {\n  font-size: 3.6rem;\n  line-height: 3.6rem;\n  padding: .6rem;\n  margin: 1.3rem;\n  background: var(--action-color);\n  color: var(--bg-color);\n}\n.form-add-note {\n  display: none;\n  padding: .3rem 1rem;\n  padding: 1rem 0;\n}\n.form-add-note input {\n  display: block;\n  padding: 0 1.6rem;\n  margin: auto;\n  max-width: 100%;\n  outline: 1px solid #333;\n}\n.form-add-note input:focus {\n  border-color: #333;\n  box-shadow: 0 0 0 inset #999, 0 0 .4rem rgba(0,0,0,.09);\n  outline: 0 none;\n}\n.note-item {\n  display: flex;\n  border-bottom: 1px solid rgba(0,0,0,.25);\n  line-height: 3rem;\n}\n.note-left {\n  display: flex;\n  align-items: center;\n  flex: 1;\n  padding-left: 1rem;\n  cursor: pointer;\n  overflow-x: auto;\n}\n.note-left p {\n  flex: 1;\n  padding: .2rem .1rem .2rem 1rem;\n  line-height: 1.8rem;\n}\n.note-left input {\n  display: inline-block;\n  vertical-align: middle;\n  text-align: center;\n}\n.btn-delete-note {\n  padding: .5rem;\n  padding-left: 1.2rem;\n  height: 100%;\n  border: 1px solid var(--action-color);\n}\n\n/*#endregion NOTES SIDEBAR */\n\n/*#endregion SIDEBAR */\n\n/*#region MAIN-CONTENT */\n.main-content {\n  flex: 3;  /* 75% width */\n  /*width: 77%;*/\n}\n\n/*#endregion MAIN-CONTENT */\n\n/*#region DASHBOARD */"},"lineCount":243}},"error":null,"hash":"57d0ec2d1cee0e80c36c0fbda473a803","cacheData":{"env":{}}}